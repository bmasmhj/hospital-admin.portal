
ErrorDocument 400 https://bimash.com.np/Error/400
ErrorDocument 401 https://bimash.com.np/Error/401
ErrorDocument 403 https://bimash.com.np/Error/403
ErrorDocument 404 https://bimash.com.np/Error/404
ErrorDocument 503 https://bimash.com.np/Error/503

RewriteEngine on

RewriteCond %{REQUEST_FILENAME} !-d

RewriteCond %{REQUEST_FILENAME}\.html -f

RewriteRule ^(.*)$ $1.html [NC,L]
RewriteRule ^Dashboard  index.php [NC,L]
RewriteRule ^Basicinfo  basicinfo.php [NC,L]
RewriteRule ^Carousel  carousel.php [NC,L]
RewriteRule ^Services  extensionservice.php [NC,L]
RewriteRule ^Provision  provision.php [NC,L]
RewriteRule ^EmergencyServices  emergency.php [NC,L]
RewriteRule ^RegularServices  regular.php [NC,L]
RewriteRule ^HealthPackageServices  healthpackages.php [NC,L]
RewriteRule ^Specialities  specialites.php [NC,L]
RewriteRule ^Appointment  bookedappoitment.php [NC,L]
RewriteRule ^Booking  helbook.php [NC,L]
RewriteRule ^Users  user.php [NC,L]
RewriteRule ^Doctors  doctor.php [NC,L]
RewriteRule ^Reception  reception.php [NC,L]
RewriteRule ^Payments  payment.php [NC,L]
RewriteRule ^Medicalreports  medicalreport.php [NC,L]
RewriteRule ^Contact  contacts.php [NC,L]
RewriteRule ^Feedback  feedback.php [NC,L]
RewriteRule ^Notification  notifictions.php [NC,L]
RewriteRule ^LOGIN  login.php [NC,L]
RewriteRule ^Logout  logout.php [NC,L]
RewriteRule ^Profile  profile.php [NC,L]










RewriteRule ^400  400.html [NC,L]
RewriteRule ^401  401.html [NC,L]
RewriteRule ^403  403.html [NC,L]
